{
	"name": "SqlToParquet",
	"properties": {
		"linkedServiceName": {
			"referenceName": "StagingRawContainer",
			"type": "LinkedServiceReference"
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
